{
  "client": {
    "insecure": true,
    "allowed_origins": ["*"]
  },
  "consumers": [
    {
      "enabled": true,
      "name": "leaderboard_redis",
      "type": "redis_stream",
      "redis_stream": {
        "address": "redis://localhost:6379",
        "streams": ["leaderboard-stream"],
        "consumer_group": "centrifugo",
        "num_workers": 8
      }
    }
  ]
}
